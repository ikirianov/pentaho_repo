	byte[] encodedKey = Base64.getDecoder().decode("1234567890123456");
	SecretKey secretKey = new SecretKeySpec(encodedKey, 0, encodedKey.length, ALGORITHM);
	String decryptedText = "";
	try{
	decryptedText = decrypt(encryptedText, secretKey);
